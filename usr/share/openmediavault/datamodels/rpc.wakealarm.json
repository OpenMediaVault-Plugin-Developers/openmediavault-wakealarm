[{
	"type": "rpc",
	"id": "rpc.wakealarm.setjob",
	"params": {
		"type": "object",
		"properties": {
			"uuid": {
				"type": "string",
				"format": "uuidv4",
				"required": true
			},
			"enable": {
				"type": "boolean",
				"required": true
			},
			"minute": {
				"type": "string",
				"pattern": "^[0-9]|1[0-9]|2[0-9]|3[0-9]|4[0-9]|5[0-9]|[*]$",
				"required": true
			},
			"everynminute": {
				"type": "boolean",
				"required": true
			},
			"hour": {
				"type": "string",
				"pattern":"^[0-9]|1[0-9]|2[0-3]|[*]$",
				"required": true
			},
			"everynhour": {
				"type":"boolean",
				"required": true
			},
			"month": {
				"type": "string",
				"pattern": "^[1-9]|1[0-2]|[*]$",
				"required": true
			},
			"dayofmonth": {
				"type": "string",
				"pattern": "^[1-9]|1[0-9]|2[0-9]|3[0-1]|[*]$",
				"required": true
			},
			"everyndayofmonth": {
				"type": "boolean",
				"required": true
			},
			"dayofweek": {
				"type": "string",
				"pattern": "^[1-7]|[*]$",
				"required": true
			},
			"comment": {
				"type": "string",
				"required": true
			}
		}
	}
}]
